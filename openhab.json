{
  "OpenHAB": {
    "description": "openHAB - a vendor and technology agnostic open source automation software for your home. Build your smart home in no time!",
    "ui": {
      "slug": ""
    },
    "website": "http://www.openhab.org/",
    "version": "1.0",
    "containers": {
      "openhab": {
        "image": "krayzee/openhab:2-beta-snapshot",
        "launch_order": 1,
        "opts": [
          [
            "-v",
            "/etc/localtime:/etc/localtime:ro",
            "-v",
            "/etc/timezone:/etc/timezone:ro"
          ]
        ],
        "uid": -1,
        "ports": {
          "8080": {
            "description": "",
            "host_default": 8080,
            "label": "Server HTTP port",
            "protocol": "tcp",
            "ui": true
          },
          "8443": {
            "description": "",
            "host_default": 8443,
            "label": "Server HTTPS port",
            "protocol": "tcp",
            "ui": false
          },
          "5555": {
            "description": "",
            "host_default": 5555,
            "label": "Server DATA port",
            "protocol": "tcp",
            "ui": false
          }
        },
        "volumes": {
          "/opt/openhab/conf": {
            "description": "Choose a Share for OpenHAB configuration.",
            "label": "OpenHAB Config"
          },
          "/opt/openhab/userdata": {
            "description": "Choose a Share for OpenHAB userdata.",
            "label": "OpenHAB Data"
          }
        }
      }
    }
  }
}