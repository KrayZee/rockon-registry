{
  "Domoticz": {
    "description": "Domoticz",
    "ui": {
      "slug": ""
    },
    "website": "http://www.domoticz.com/",
    "version": "1.0",
    "containers": {
      "domoticz": {
        "image": "sdesbure/domoticz",
        "launch_order": 1,
        "opts": [
          [
            "--net=host",
            "",
            "-v",
            "/etc/localtime:/etc/localtime:ro",
            "-v",
            "/etc/timezone:/etc/timezone:ro"
          ]
        ],
        "uid": -1,
        "ports": {
          "8080": {
            "description": "",
            "host_default": 8080,
            "label": "Server HTTP port",
            "protocol": "tcp",
            "ui": true
          }
        },
        "volumes": {
          "/config": {
            "description": "Choose a Share for Domoticz configuration.",
            "label": "Domoticz Config"
          }
        }
      }
    }
  }
}